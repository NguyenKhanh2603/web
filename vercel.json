{
  "version": 2,
  "trailingSlash": false,
  "cleanUrls": false,
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ],
  "redirects": [
    { "source": "/BLOGS", "destination": "/BLOGS/blogs.html", "permanent": false },
    { "source": "/BLOGS/", "destination": "/BLOGS/blogs.html", "permanent": false },
    { "source": "/PEOPLE", "destination": "/PEOPLE/people.html", "permanent": false },
    { "source": "/PEOPLE/", "destination": "/PEOPLE/people.html", "permanent": false }
  ]
}